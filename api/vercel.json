{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node@3"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/send-hockey-reminders",
      "schedule": "0 16 * * *"
    }
  ]
}
